{
  "name": "pdf-to-svg-html",
  "version": "1.0.0",
  "description": "PDF to SVG converter - Integrated Frontend & Backend",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "client": "cd client && npm run serve",
    "client:build": "cd client && npm run build",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "cd client && npm run build",
    "postinstall": "cd client && npm install"
  },
  "keywords": [
    "pdf",
    "svg",
    "converter"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "multer": "^1.4.4",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "concurrently": "^8.0.0"
  }
}
